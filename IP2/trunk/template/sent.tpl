<html>
	<head>
		<title>Your email has been sent</title>
		<link href="../style/style.css" rel="stylesheet" type="text/css" />
	</head>
	<body>
		<!!--error-->
			<div class="head">
				Error
			</div>
			<hr/>
			<div class="errorText">
				<!--error-message-->
			</div>
		<!!--error-->
		<!!--sent-->
			<div class="head">
				Email sent!
			</div>
			<hr/>
			<table>
				<tr>
					<td>
						To
					</td>
					<td>
						<!--to-->
					</td>
				</tr>
				<tr>
					<td>
						Cc
					</td>
					<td>
						<!--cc-->
					</td>
				</tr>
				<tr>
					<td>
						Bcc
					</td>
					<td>
						<!--bcc-->
					</td>
				</tr>
				<tr>
					<td>
						From
					</td>
					<td>
						<!--from-->
					</td>
				</tr>
				<tr>
					<td>
						Subject
					</td>
					<td>
						<!--subject-->
					</td>
				</tr>
				<tr>
					<td colspan="2">
						Message
					</td>
				</tr>
				<tr>
					<td colspan="2">
						<pre>
							<!--message-->
						</pre>
					</td>
				</tr>
			</table>
		<!!--sent-->
		<p>
		<a href="/dsv/send.html">Back</a>
	</body>
</html>